{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ValueExtractorPayload",
  "type": "object",
  "properties": {
    "collection_name": {
      "type": "string",
      "description": "Name of the collection."
    },
    "id": {
      "description": "Unique identifier, either a UUID string or a positive integer.",
      "oneOf": [
        {
          "type": "string",
          "format": "uuid",
          "description": "UUID string as the identifier."
        },
        {
          "type": "integer",
          "minimum": 0,
          "description": "Positive integer as the identifier."
        }
      ]
    },
    "vector": {
      "type": "array",
      "description": "Vector data associated with the point.",
      "items": {
        "type": "number"
      }
    },
    "payload": {
      "description": "Additional payload data.",
      "anyOf": [
        {
          "type": "object",
          "description": "Payload as an object.",
          "properties": {
            "doi": {
              "type": "string"
            },
            "title": {
              "type": "string"
            }
          },
          "additionalProperties": true
        },
        {
          "type": "null",
          "description": "Payload can be null."
        }
      ]
    }
  },
  "required": [
    "collection_name",
    "id"
  ],
  "additionalProperties": false
}
